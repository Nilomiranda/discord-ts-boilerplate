{
  "name": "discord-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Danilo Miranda",
  "license": "MIT",
  "scripts": {
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "dev": "node dist/index.js",
    "watch": "rm -rf dist && tsc-watch --onSuccess 'yarn dev'"
  },
  "dependencies": {
    "ascii-table": "^0.0.9",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "jsdom": "^16.5.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.7",
    "@types/node": "^14.14.36",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "husky": "4",
    "lint-staged": ">=10",
    "prettier": "2.2.1",
    "simple-git-hooks": ">=2.0.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
